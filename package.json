{
  "name": "drone-mobile-sdk",
  "version": "1.0.1",
  "description": "An unofficial Node.js and TypeScript library for interacting with the DroneMobile API. Remotely control and monitor your vehicle with ease.",
  "keywords": [
    "drone-mobile",
    "sdk",
    "typescript",
    "aws",
    "bun"
  ],
  "homepage": "https://github.com/jaredthejellyfish/drone-mobile#readme",
  "bugs": {
    "url": "https://github.com/jaredthejellyfish/drone-mobile/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jaredthejellyfish/drone-mobile.git"
  },
  "license": "MIT",
  "author": "Jared Norton",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target node --no-splitting --minify",
    "build:types": "bun x tsc",
    "prepublishOnly": "bun run build && bun run build:types",
    "test": "bun test"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.665.0",
    "dotenv": "^16.4.5",
    "got": "^14.4.2",
    "pino": "^9.4.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.0.0"
  }
}